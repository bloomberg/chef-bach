{
  "name": "Test-Laptop",
  "override_attributes": {
    "bcpc": {
      "domain_name" : "bcpc.example.com",
      "management": {
        "vip" : "10.0.100.5"
      },
      "floating": {
        "vip" : "192.168.100.5"
      },
      "networks": {
        "subnet1": {
          "management": {
            "vip" : "10.0.100.5",
            "interface" : "eth0",
            "netmask" : "255.255.255.0",
            "cidr" : "10.0.100.0/24",
            "gateway" : "10.0.100.3"
          },
          "storage": {
            "interface" : "eth1",
            "netmask" : "255.255.255.0",
            "cidr" : "172.16.100.0/24",
            "gateway" : "172.16.100.3"
          },
          "floating": {
            "vip" : "192.168.100.5",
            "interface" : "eth2",
            "netmask" : "255.255.255.0",
            "cidr" : "192.168.100.0/24",
            "gateway" : "192.168.100.3",
            "available_subnet" : "192.168.100.128/25"
          }
        }
      },
      "bootstrap": {
        "interface" : "eth0",
        "pxe_interface" : "eth1",
        "hostname" : "bcpc-bootstrap",
        "server" : "10.0.100.3",
        "dhcp_subnet" : "10.0.100.0",
        "dhcp_range" : "10.0.100.14 10.0.100.250"
      },
      "dns_servers" : [ "8.8.8.8", "8.8.4.4" ],
      "repos" : {
        "hortonworks": "http://public-repo-1.hortonworks.com/HDP/ubuntu12/2.x/updates/2.3.4.0",
        "hdp_utils": "http://public-repo-1.hortonworks.com/HDP-UTILS-1.1.0.20/repos/ubuntu12"
      },
      "hadoop" : {
        "yarn" : {
          "resourcemanager" : {
            "yarn.client.failover-sleep-base-ms": 30000
          }
        },
        "hdfs" : {
          "dfs_replication_factor" : 1
        },
        "distribution": {
          "version": "HDP",
          "key": "hortonworks.key",
          "release": "2.3.4.0-3485"
        },
        "hbase": {
          "bucketcache": {
            "enabled": false
          },
          "repl": {
            "enabled": false
          },
          "repl": {
            "target": ""
          },
          "shortcircuit": {
            "read": false
          },
          "region": {
            "replication": {
              "enabled": false
            }
          },
          "ipc": {
            "warn": {
              "responsetime": 300
            }
          }          
        },
        "hbase_rs": {
          "xmn": {
            "size": 256
          },
          "xms": {
            "size": 1024
          },
          "xmx": {
            "size": 1024
          },
          "mx_dir_mem": {
            "size": 256
          },
          "hdfs_dir_mem": {
            "size": 128
          }
        },
        "oozie": {
          "memory_opts": "-Xmx512m -XX:MaxPermSize=64m"
        }
      }
    },
    "kafka": {
      "version": "0.8.1.1",
      "scala_version": "2.9.2",
      "checksum": "",
      "md5_checksum": "",
      "log_dir": "/disk/0"
    },
    "ntp": {
       "servers" : [ "0.pool.ntp.org", "1.pool.ntp.org", "2.pool.ntp.org", "3.pool.ntp.org" ]
    },
    "chef_client": {
      "server_url": "http://10.0.100.3:4000",
      "cache_path": "/var/chef/cache",
      "config": {
         "verify_api_cert": false
      },
      "backup_path": "/var/chef/backup",
      "validation_client_name": "chef-validator",
      "run_path": "/var/chef"
    }
  },
  "json_class": "Chef::Environment",
  "description": "Laptop test environment",
  "cookbook_versions": {
  },
  "chef_type": "environment"
}
