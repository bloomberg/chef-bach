UserParameter=ceph.rgw.bucket.objects[*],HOME=/var/lib/zabbix radosgw-admin bucket stats --bucket='$1' | python -c "import json, sys; print json.load(sys.stdin)['usage']['rgw.main']['num_objects']"
 



